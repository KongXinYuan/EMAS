public class NodeStatusExpand extends TNodeStatus {

	/**
	 * @field serialVersionUID
	 * @field_type long
	 */
	private static final long serialVersionUID = -2600132386732201226L;

	
	private String fpath;
	
	private int fwidth;
	
	private int fheight;
	
	private String fname;
	
	private int fposition;
	
	private String ftype;
	
	private String fdefinition;

	public String getFpath() {
		return fpath;
	}

	public void setFpath(String fpath) {
		this.fpath = fpath;
	}

	public int getFwidth() {
		return fwidth;
	}

	public void setFwidth(int fwidth) {
		this.fwidth = fwidth;
	}

	public int getFheight() {
		return fheight;
	}

	public void setFheight(int fheight) {
		this.fheight = fheight;
	}

	public String getFname() {
		return fname;
	}

	public void setFname(String fname) {
		this.fname = fname;
	}

	public int getFposition() {
		return fposition;
	}

	public void setFposition(int fposition) {
		this.fposition = fposition;
	}

	public String getFtype() {
		return ftype;
	}

	public void setFtype(String ftype) {
		this.ftype = ftype;
	}

	public String getFdefinition() {
		return fdefinition;
	}

	public void setFdefinition(String fdefinition) {
		this.fdefinition = fdefinition;
	}
	
